{
	"name": "Dockerfile",
	"image": "ghcr.io/wagov/siem-query-utils:main",
	"hostRequirements": {
		"cpus": 4
	},
	"updateContentCommand": "poetry install",
	"customizations": {
		"codespaces": {
			"repositories": {
				"my_org/*": {
					"permissions": {
						"permissions": "write-all"
					}
				}
			}
		},
		"vscode": {
			"extensions": [
				"ms-toolsai.jupyter",
				"ms-python.python"
			]
		}
	},
	"mounts": [
		"source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
	]
}