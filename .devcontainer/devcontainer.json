{
	"name": "Python 3.10 (Project)",
	"image": "mcr.microsoft.com/devcontainers/python:3.10",
	"features": {
		"ghcr.io/devcontainers/features/github-cli": {},
		"ghcr.io/devcontainers/features/docker-in-docker": {}
	},
	"hostRequirements": {
		"cpus": 4
	},
	"customizations": {
		"codespaces": {
			"repositories": {
				"wagov/wasoc-notebook": {
					"permissions": {
						"permissions": "write-all"
					}
				}
			}
		},
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter",
				"GitHub.copilot"
			]
		}
	},
	"onCreateCommand": [
		"/bin/bash",
		"-i",
		"install.sh"
	],
	"updateContentCommand": [
		"/bin/bash",
		"-i",
		"update.sh"
	],
	"forwardPorts": [
		8000,
		8888
	],
	"portsAttributes": {
		"3000": {
			"label": "atlaskit"
		},
		"8000": {
			"label": "API Server"
		},
		"8888": {
			"label": "Jupyter Lab"
		}
	}
}