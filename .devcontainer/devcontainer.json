{
	"name": "Python 3.10 SQU V2.0.0",
	"build": {
		"dockerfile": "../Dockerfile",
		"cacheFrom": " ghcr.io/wagov/siem-query-utils:latest"
	},
	"features": {
		"ghcr.io/devcontainers/features/github-cli": {},
		"ghcr.io/devcontainers/features/docker-in-docker": {}
	},
	"hostRequirements": {
		"cpus": 4
	},
	"containerEnv": {
		"PATH": "${localEnv:PATH}:.venv/bin"
	},
	"customizations": {
		"codespaces": {
			"repositories": {
				"wagov/wasoc-notebook": {
					"permissions": "write-all"
				}
			}
		},
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter",
				"GitHub.copilot"
			]
		}
	},
	"updateContentCommand": [
		"/bin/bash",
		"-i",
		"update.sh"
	],
	"forwardPorts": [
		8000,
		8888
	],
	"portsAttributes": {
		"3000": {
			"label": "atlaskit"
		},
		"8000": {
			"label": "API Server"
		},
		"8888": {
			"label": "Jupyter Lab"
		}
	}
}
